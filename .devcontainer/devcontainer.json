{
  "name": "Terraform AWS CLI Docker",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "docker-in-docker": {}
  },
  "hostRequirements": {
    "cpus": 4,
    "memory": "8gb"
  },
  "postCreateCommand": "terraform --version && aws --version && docker --version",
  "remoteUser": "vscode"
}
